		<fastjson.version>1.2.30</fastjson.version>
		<jackson.version>2.9.0</jackson.version>
		<springfox-swagger.version>2.6.1</springfox-swagger.version>
		<redis-lock.version>1.0.4.RELEASE</redis-lock.version>
	</properties>

